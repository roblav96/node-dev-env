{
	"name": "node-env-dev",
	"version": "1.2.2",
	"description": "Get woke with a better Node.js development environment",
	"main": "dist/index.js",
	"files": [
		"dist"
	],
	"bin": {
		"node-env-dev": "dist/index.js"
	},
	"scripts": {
		"clean": "del dist node_modules",
		"prepublishOnly": "del dist && tsc",
		"start": "del dist && tsc; node dist/index.js",
		"watch": "del dist && onchange --verbose --kill dist/**/*.js -- node dist/index.js & tsc --watch"
	},
	"dependencies": {
		"source-map-support": "^0.5.12"
	},
	"devDependencies": {
		"@types/node": "^11.13.13",
		"del-cli": "^2.0.0",
		"onchange": "^6.0.0",
		"typescript": "^3.5.1"
	},
	"keywords": [
		"catcher",
		"catching",
		"console",
		"dev",
		"development",
		"env",
		"environment",
		"error",
		"log",
		"pretty",
		"woke"
	],
	"license": "Unlicense",
	"author": "Robert Laverty <roblav96@gmail.com> (https://github.com/roblav96)",
	"homepage": "https://github.com/roblav96/node-env-dev#readme",
	"repository": "github:roblav96/node-env-dev",
	"bugs": "https://github.com/roblav96/node-env-dev/issues"
}
